{
    "name": "digitalcloud/nova-page-tool",
    "description": "A Laravel Nova Page tool.",
    "keywords": [
        "laravel",
        "nova",
        "tool",
        "page builder",
        "page design"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0",
		  "spatie/laravel-sluggable": "^3.0.0",
			"infinety-es/nova-filemanager": "^2.8",
		"digitalcloud/nova-page-builder-field": "^1.0",
		"digitalcloud/multilingual-nova": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalCloud\\PageTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DigitalCloud\\PageTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
